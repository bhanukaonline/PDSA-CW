<Window x:Class="PDSA_Games.QueensPuzzle"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PDSA_Games"
        mc:Ignorable="d"
        Title="QueensPuzzle" Height="450" Width="800">
    <Grid>
        <Label Content="8 Queens Puzzle" HorizontalAlignment="Center" Height="41" Margin="0,21,0,0" VerticalAlignment="Top" Width="168" FontSize="18" FontFamily="Cascadia Code SemiBold"/>
        <Button x:Name="startGame" Content="Start Game" HorizontalAlignment="Center" Height="55" Margin="0,199,0,0" VerticalAlignment="Top" Width="114" FontFamily="Cascadia Code" FontSize="16" Click="startGame_Click"/>
        <Button Content="All Solutions" HorizontalAlignment="Center" Height="55" Margin="0,259,0,0" VerticalAlignment="Top" Width="114" Click="Button_Click" RenderTransformOrigin="2.074,1.539" FontSize="14" FontFamily="Cascadia Code"/>
        <Button x:Name="Menu" Content="Main Menu" HorizontalAlignment="Center" Height="46" Margin="0,340,0,0" VerticalAlignment="Top" Width="114" FontSize="18" FontFamily="Cascadia Code" Click="Menu_Click"/>

    </Grid>
</Window>
